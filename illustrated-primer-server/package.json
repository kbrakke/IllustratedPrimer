{
  "name": "illustrated-primer-server",
  "version": "0.0.1",
  "description": "A young lady's illustrated primer",
  "main": "dist/index.js",
  "scripts": {
    "db:generate": "primsa generate",
    "db:seed": "prisma db seed",
    "debug": "NODE_ENV=development nodemon src/app.ts",
    "dev": "NODE_ENV=development ./node_modules/.bin/pm2 start src/app.ts --name illustrated-primer --watch",
    "build": "tsc",
    "lint": "eslint --ext .ts src",
    "start": "pm2 start ts-node -- dist/app.js --name illustrated-primer --watch",
    "test": "jest",
    "postinstall": "./node_modules/.bin/pm2 install typescript"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^4.9.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "openai": "^3.2.1",
    "pino": "^8.11.0",
    "pino-http": "^8.3.3"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.7.13",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.9.1",
    "eslint": "^8.36.0",
    "eslint-config-airbnb-typescript": "^16.0.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^25.4.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "jest": "^27.4.3",
    "nodemon": "^2.0.22",
    "pm2": "^5.1.3",
    "prisma": "^4.9.0",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "prisma": {
    "schema": "./prisma/schema.prisma",
    "seed": "ts-node prisma/seed.ts"
  }
}
